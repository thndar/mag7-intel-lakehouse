###############################################################################
# ENVIRONMENT TEMPLATE FOR MAG7-INTEL
# Copy this file to `.env`.
###############################################################################

# ============================================================================
# EXTRACTiON SETTINGS
# ============================================================================
# Total years for historical backfill
BACKFILL_YEARS=5

# Optional: override the start date for backfill (YYYY-MM-DD)
# If unset, START_DATE = TODAY - BACKFILL_YEARS
START_DATE=2020-01-01

# Magnificent 7 default tickers (comma-separated)
TICKERS=AAPL,MSFT,GOOGL,AMZN,META,TSLA,NVDA

# NASDAQ index tickers we want to track
INDICES=^IXIC,^NDXE

# extracted data output directory (tap for meltano)
OUTPUT_DIR=./data

# ============================================================================
# BIGQUERY DATASETS
# ============================================================================
# Shared datasets used by the main Dagger/CI pipeline
BQ_DATASET_PRIX=mag7_intel      # prefix for all datasets
BQ_DATASET_RAW=mag7_intel_raw
BQ_DATASET_STAGING=mag7_intel_staging
BQ_DATASET_INTERMEDIATE=mag7_intel_intermediate
BQ_DATASET_MART=mag7_intel_mart
